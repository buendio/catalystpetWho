.cl-litter-products {
  display: flex;
  position: relative;
  width: 100%;
  min-height: 610px;

  .arrow-down {
    position: absolute;
    left: 0;
    right: 0;
    top: -22.5px;
    background: #DDAD26;
    width: 45px;
    height: 45px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    margin: auto;

    &::before {
      content: '';
      position: absolute;
      border: 3px #027747 solid;
      border-top: 0;
      border-left: 0;
      transform: rotate(45deg);
      width: 18px;
      height: 18px;
      top: 0;
      left: 1px;
      bottom: 8px;
      right: 0;
      margin: auto;
    }
  }

  .image-left {
    width: 50%;
    background-size: cover;
    background-repeat: no-repeat;
  }
  .center-image {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    max-width: 420px;
    display: none;

    &.selected {
      display: block;
    }

    @media(max-width: 1550px) and (min-width: 1401px) {
      max-width: 330px;
    }

    @media(max-width: 1400px) {
      max-width: 260px;
    }

    @media(max-width: 1400px) {
      max-width: 260px;
    }
    @media(max-width: 1200px) {
      max-width: 200px;
    }

    @media(max-width: 991px) and (min-width: 678px) {
      left: -180px;
    }
  }
  .products-right {
    width: 50%;
    display: flex;
    justify-content: center;
    align-items: center;

    .wrapper {
      max-width: 350px;


      @media(max-width: 1200px) and (min-width: 992px) {
        margin-let: 50px;
      }

      @media(max-width: 991px) and (min-width: 678px) {
        position: relative;
        right: 0;
      }

      h3 {
        color: #5D5D5D;
        font-family: "Domine";
        font-size: 24px;
        line-height: 28px;
        text-transform: none;
        font-weight: 300;
        letter-spacing: normal;
        width: 80%;
      }

      ul {
        margin: 35px 0 50px;

        li {
          a {
            color: #5D5D5D;
            font-family: "Futura", sans-serif;
            font-size: 26px;
            font-weight: bold;
            line-height: 35px;
            text-transform: uppercase;
            outline: none;

            p {
              margin: 0;
              display: inline-block;
            }

            span {
              width: 32px;
              height: 32px;
              margin: 0 5px -7px 0;
              display: inline-block;
              border: 1px solid #d7d7d7;
              -webkit-border-radius: 50%;
              -moz-border-radius: 50%;
              border-radius: 50%;
              position: relative;

              &::before {
                content: '';
                position: absolute;
                width: 24px;
                height: 24px;
                top: 3px;
                left: 3px;
                -webkit-border-radius: 50%;
                -moz-border-radius: 50%;
                border-radius: 50%;
              }
            }

            &.selected {
              color: #027747;
              span {
                &::before {
                  background: #027747;
                }
              }
            }

            &[data-product="orange"].selected {
              color: #DD6235;
              span {
                &::before {
                  background: #DD6235;
                }
              }
            }
            &[data-product="blue"].selected {
              color: #3F9BC9;
              span {
                &::before {
                  background: #3F9BC9;
                }
              }
            }
          }
        }
      }

      .selection {
        color: #027747;
        font-family: Futura, sans-serif;
        font-size: 20.48px;
        font-weight: bold;
        line-height: 27px;
        text-align: center;
        text-transform: uppercase;
        display: none;

        &[data-product="orange"] {
          color: #DD6235;
        }

        &[data-product="blue"] {
          color: #3F9BC9;
        }
      }

      a.try-catalyst {
        text-transform: uppercase;
        background-color: #DDAD26;
        width: 100%;
        max-width: unset;
        display: inline-block;
        text-align: center;
        font-size: 14px;

        .try-catalyst-label {
          font-family: "Futura";
          font-size: 16px;
        }

        &.disabled {
          filter: grayscale(100%);
          pointer-events: none;
        }
      }
    }
  }

  @media(max-width: 719px) {
    flex-wrap: wrap;
    min-height: unset;

    .image-left {
      width: 100%;
      height: 375px;
    }
    .products-right {
      width: 100%;
      min-height: 375px;

      .wrapper {
        max-width: unset;
        width: 90%;
        text-align: center;
        margin: 160px 0 0;

        h3 {
          font-size: 16px;
          line-height: 19px;
          width: 100%;
          margin: 0;
        }
        ul {
          margin: 15px 0;
          li {
            display: inline-block;

            a p {
              display: none;
            }
          }
        }
        .selection {
          display: block;
          margin: 0 0 40px;
        }
        .try-catalyst {
          width: 100%;
          max-width: 290px;
          display: inline-block;
        }
      }
    }
    .center-image {
      max-width: 260px;
      top: -130px;
    }
  }
}

