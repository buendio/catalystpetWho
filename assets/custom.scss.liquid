/*============================================================================
  #Drawers
==============================================================================*/
$zindexDrawer: 10;
$drawerNavWidth: 300px;
$drawerCartWidth: 375px;
$colorDrawers: #f6f6f6;
$colorDrawerBorder: #DDAD26;
$colorDrawerText: #333;
$drawerTransition: all 0.4s cubic-bezier(0.46, 0.01, 0.32, 1);

.js-drawer-open {
  overflow: hidden;
}

.drawer {
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  top: 0;
  bottom: 0;
  padding: 0 40px 25px;
  background: #fff;
  z-index: 999;
}

.drawer--left {
  width: $drawerNavWidth;
  left: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;

  .js-drawer-open-left & {
    display: block;
    transform: translateX($drawerNavWidth);

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;

  .js-drawer-open-right & {
    display: block;
    transform: translateX(-$drawerCartWidth);

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  overflow: hidden;
}

.is-moved-by-drawer {
  transition: $drawerTransition;

  .js-drawer-open-left & {
    transform: translateX($drawerNavWidth);
  }

  .js-drawer-open-right & {
    transform: translateX(-$drawerCartWidth);
  }
}

.drawer__header {
  position: relative;
  height: 86px;
  width: 100%;
  border-bottom: 16px solid $colorDrawerBorder;
}

.drawer__title {
  width: 100%;
  color: #5D5D5D;
  font-family: Montserrat;
  font-size: 21px;
  font-weight: 900;
  letter-spacing: 1px;
  line-height: 86px;
  text-align: center;

  .CartCount {
    font-size: 15px;
    position: relative;
    top: -10px;
  }
}

.drawer__close {
  text-align: center;
  position: absolute;
  color: #000000;
  top: 0;
  left: -17px;
  font-size: 30px;
  font-weight: normal;
  opacity: .8;
  font-family: "Montserrat", sans-serif;

  button {
    border: 0;
    padding: 0;
    margin: 0;
    background: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
  }
}
