            border: 1px solid #d7d7d7;
            -webkit-border-radius: 50%;
            -moz-border-radius: 50%;
            border-radius: 50%;
            position: relative;

            &::before {
              content: '';
              position: absolute;
              width: 32px;
              height: 32px;
              top: 4px;
              left: 4px;
              -webkit-border-radius: 50%;
              -moz-border-radius: 50%;
              border-radius: 50%;
            }
          }

          &.selected {
            color: #027747;
            span {
              &::before {
                background: #027747;
              }
            }
          }

          &[data-product="orange"].selected {
            color: #DD6235;
            span {
              &::before {
                background: #DD6235;
              }
            }
          }
          &[data-product="blue"].selected {
            color: #3F9BC9;
            span {
              &::before {
                background: #3F9BC9;
              }
            }
          }
        }
      }
    }
  }
